{
    "coc.preferences.formatOnSaveFiletypes": ["elm", "javascript", "typescript", "typescriptreact", "json", "javascriptreact","css", "Markdown"],
    "eslint.filetypes": ["javascript", "typescript", "typescriptreact", "javascriptreact"],
    "coc.preferences.codeLens.enable": true,
    "coc.preferences.currentFunctionSymbolAutoUpdate": true,
    "coverage.uncoveredSign.text":"▌",
    "diagnostic.errorSign": "▌",
    "diagnostic.warningSign": "▌",
    "diagnostic.infoSign": "▌",
    "explorer.width": 30,
    "explorer.icon.enableNerdfont": true,
    "explorer.previewAction.onHover": false,
    "explorer.keyMappings.global": {
      "<cr>": ["expandable?", "expand", "open"],
      "v": "open:vsplit"
    },
    "languageserver": {
      "elmLS": {
        "command": "elm-language-server",
        "filetypes": ["elm"],
        "rootPatterns": ["elm.json"],
        "initializationOptions": {
          "elmAnalyseTrigger": "change"
        }
      }
    }
}
